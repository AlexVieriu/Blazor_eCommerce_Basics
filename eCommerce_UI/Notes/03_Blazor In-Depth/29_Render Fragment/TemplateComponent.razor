@typeparam TList

@Header
<div>
    @foreach (var item in Items)
    {
        @RepeatFrag(item)
    }
</div>
@Footer

@code{
    [Parameter]
    public RenderFragment Header { get; set; }

    [Parameter]
    public RenderFragment Footer { get; set; }

    [Parameter]
    public List<TList> Items { get; set; }

    [Parameter]
    public RenderFragment<TList> RepeatFrag { get; set; }
}